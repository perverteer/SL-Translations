    # h "Here's my number in case you need it."
    h "Здесь мой номер, если он вам нужен."

# game/day10.rpy:580
translate russian day10_dwayne_visit_6bfa0dd5:

    # p "{i}(Eager, eager Hazel...)"
    p "{i}(Нетерпеливая, нетерпеливая Хэйзел...)"

