translate russian day10_dwayne_visit_752ba8bc:

    # p "That ah..."
    p "Это..."

# game/day10.rpy:570
translate russian day10_dwayne_visit_1d6f329e:

    # p "There he is!"
    p "Это он!"

# game/day10.rpy:571
translate russian day10_dwayne_visit_865cc3d3:

