    p "{i}(...)"

# game/day23.rpy:869
translate german day23_m_restaurant_secondary_choices_9f0bd217:

    # ir "Oh, [p_name]?"
    ir "Oh, [p_name]?"

# game/day23.rpy:870
translate german day23_m_restaurant_secondary_choices_be38147e:

    # p "{i}(Bingo!)"
    p "{i}(Bingo!)"

# game/day23.rpy:871
translate german day23_m_restaurant_secondary_choices_0d6c348f:

    # p "Yes, Iris?"
    p "Ja, Iris?"

# game/day23.rpy:873
translate german day23_m_restaurant_secondary_choices_3ec7b14e:

    # ir "I thought it would be good if we exchange phone numbers, should I decide to take you up on your offer."
    ir "Ich dachte, es wäre gut, wenn wir Telefonnummern austauschen, falls ich mich entschließen sollte, dein Angebot anzunehmen."

# game/day23.rpy:874
translate german day23_m_restaurant_secondary_choices_fd009721:

