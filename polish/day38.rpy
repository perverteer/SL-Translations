# game/day38.rpy:62
translate polish day38_e65db352:

    # p "No, I'm not joking, I'm very serious."
    p "Nie, nie żartuję, mówię bardzo poważnie."

# game/day38.rpy:63
translate polish day38_1068d078:

    # p "I very much want this for us."
    p "Bardzo tego dla nas chcę."

# game/day38.rpy:64
