translate spanish day23_m_restaurant_secondary_choices_7799c5e7:

    # ir "Yes, I thought I saw you with a woman earlier."
    ir "Sí, pensé que te había visto antes con una mujer."

# game/day23.rpy:833
translate spanish day23_m_restaurant_secondary_choices_204e4aa5:

    # p "Yes, she's my mother."
    p "Sí, ella es mi madre."

# game/day23.rpy:835
translate spanish day23_m_restaurant_secondary_choices_93795a7d:

    # p "Yes, she's a friend."
    p "Sí, es una amiga."

# game/day23.rpy:837
translate spanish day23_m_restaurant_secondary_choices_4ce7fda4:

